{
  "name": "vcgi-map-performance-monitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ext .ts,.tsx,.js --max-warnings=0 || true",
    "build": "vite build && node ./scripts/patch-dist-arcgis-assets.cjs",
    "build:prod": "cross-env VITE_BASE='./' vite build",
    "preview": "vite preview",
    "copy-arcgis-assets": "node ./scripts/copy-arcgis-assets.cjs",
    "postinstall": "npm run copy-arcgis-assets",
    "serve-dist": "node ./scripts/serve-dist.cjs"
  },
  "serveConfig": {
    "distPath": "./dist",
    "mountPath": "/websites/dist"
  },
  "dependencies": {
    "react-dom": "^19.2.0",
    "react": "^19.2.0",
    "lucide-react": "^0.554.0",
    "@google/genai": "^1.30.0",
    "@arcgis/core": "latest"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "tailwindcss": "^3.4.8",
    "postcss": "^8.4.21",
    "autoprefixer": "^10.4.14",
    "express": "^4.18.2"
  }
}
